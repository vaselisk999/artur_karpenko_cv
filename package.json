{
  "name": "cv",
  "version": "1.0.0",
  "description": "CV",
  "main": "index.js",
  "scripts": {
    "test": "jest --config=./jest.js --coverage",
    "lint": "npm run lint:js && npm run lint:sass",
    "lint:js": "eslint --ext .js --ext .jsx .",
    "lint:sass": "sass-lint --verbose --no-exit",
    "bundle": "parcel build ./index.js ./style.scss --out-dir ./assets"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ArturK/cv.git"
  },
  "keywords": [
    "CV"
  ],
  "author": "Artur Karpenko",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ArturK/cv/issues"
  },
  "homepage": "https://github.com/ArturK/cv#readme",
  "eslintConfig": {
    "extends": [
      "airbnb",
      "plugin:jest/recommended"
    ],
    "parser": "babel-eslint",
    "env": {
      "browser": true,
      "node": true,
      "serviceworker": true
    },
    "rules": {
      "react/forbid-prop-types": false,
      "react/no-array-index-key": false,
      "import/no-extraneous-dependencies": [
        "error",
        {
          "devDependencies": true
        }
      ],
      "react/jsx-filename-extension": [
        1,
        {
          "extensions": [
            ".js",
            ".jsx"
          ]
        }
      ]
    }
  },
  "eslintIgnore": [
    "node_modules",
    "assets",
    "coverage",
    "jest.js"
  ],
  "sasslintConfig": "./config/sass-lint.yml",
  "babel": {
    "presets": [
      "env",
      "react"
    ]
  },
  "sass": {
    "includePaths": [
      "node_modules"
    ]
  },
  "browserslist": [
    "> 2%",
    "last 2 version",
    "not dead"
  ],
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.7.0",
    "eslint": "^5.9.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^22.1.2",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "jest": "^23.6.0",
    "parcel-bundler": "^1.10.3",
    "sass": "^1.15.1",
    "sass-lint": "^1.12.1",
    "uninstall": "0.0.0"
  },
  "dependencies": {
    "normalize.css": "^8.0.1",
    "prop-types": "^15.6.2",
    "react": "^16.6.3",
    "react-dom": "^16.6.3"
  }
}
